<map_types>
	<!-- Format:
		<type
				name="Type name"
				pattern="pattern in Lua format"
				ghostmode="enable ghostmode"
				max_others_in_row="other maps in row allowed"
				max_fps="maximal FPS"
				winning_vehicles="id1,id2"
				disabled_shop_items="item1,item2" />
	-->
	<type name="Race" pattern="%[race%]" ghostmode="true" max_others_in_row="6" max_fps="60"
		disabled_shop_items="mine,oil,thunder,smoke,spikestrip" />
	<type name="DM" pattern="%[dm%]" ghostmode="true" max_others_in_row="10" max_fps="53"
		winning_vehicles="520,425,464,447"
		disabled_shop_items="mine,oil,thunder,smoke,spikestrip" /> <!-- winning_vehicles: hunter, hydra, rc baron, seasparrow -->
	<type name="DD" pattern="%[dd%]" ghostmode="false" max_others_in_row="10" max_fps="60" />
	<type name="Other" ghostmode="false" max_others_in_row="20" max_fps="60" />
</map_types>